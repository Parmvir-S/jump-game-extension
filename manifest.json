{
    "manifest_version": 2,
    "name": "Jump Game",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "permissions": [
        "tabs", "<all_urls>"
      ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}